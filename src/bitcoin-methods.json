{
  "getAccount": {
    "description": "The getaccount RPC returns the name of the account associated with the given address.",
    "spec": "https://bitcoin.org/en/developer-reference#getaccount",
    "params": {
      "address": {
        "type": "string",
        "required": true,
        "description": "A P2PKH or P2SH Bitcoin address belonging either to a specific account or the default account (“”)"
      }
    },
    "result": {
      "type": "string",
      "required": true,
      "description": "The name of an account, or an empty string (“”, the default account)"
    }
  },
  "getWalletInfo": {
    "description": "The getwalletinfo RPC provides information about the wallet.",
    "spec": "https://bitcoin.org/en/developer-reference#getwalletinfo",
    "params": {},
    "result": {
      "type": {
        "walletversion": 0,
        "balance": 0,
        "txcount": 0,
        "keypoololdest": 0,
        "keypoolsize": 0,
        "unlocked_until": 0
      },
      "required": true,
      "description": ""
    }
  },
  "getBlockChainInfo": {
    "description": "The getblockchaininfo RPC provides information about the current state of the block chain.",
    "spec": "https://bitcoin.org/en/developer-reference#getblockchaininfo",
    "params": {},
    "result": {
      "type": {
        "chain": "string",
        "blocks": 0,
        "headers": 0,
        "bestblockhash": "string",
        "difficulty": 0,
        "mediantime": 0,
        "verificationprogress": 0,
        "chainwork": "string",
        "pruned": true,
        "softforks": [{
          "id": "string",
          "version": 0,
          "reject": {
            "status": true
          }
        }],
        "bip9_softforks": {
          "csv": {
            "status": "string",
            "startTime": 0,
            "timeout": 0,
            "since": 0
          },
          "segwit": {
            "status": "string",
            "bit": 0,
            "startTime": 0,
            "timeout": 0,
            "since": 0
          }
        }
      },
      "required": true,
      "description": ""
    }
  }
}